{
  "name": "ts-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate code for Node typescript",
  "main": "server.js",
  "scripts": {
    "build-ts": "rm -rf dist && tsc",
    "start": "npm run serve",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "rm -rf dist && mkdir dist && mkdir dist/views && cp -r src/views/* dist/views/ && tsc -w",
    "fix-lint": "./node_modules/.bin/tslint --fix -c ./tslint.json './src/**/*.ts'",
    "build-dev": " webpack --config webpack.dev.js",
    "dev": "webpack --config webpack.dev.js --watch",
    "seed-undo": "node_modules/.bin/sequelize db:seed:undo:all",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate-test": "NODE_ENV=test npm run migrate",
    "migrate-undo": "node_modules/.bin/sequelize db:migrate:undo:all",
    "seed-all": "node_modules/.bin/sequelize db:seed:all",
    "prod": "NODE_ENV=production webpack --config webpack.prod.js",
    "test": "NODE_ENV=test node_modules/.bin/mocha dist/test/ --recursive --exit"
  },
  "author": "",
  "license": "ISC",
